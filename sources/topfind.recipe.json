
{
"sources" : {
  "files" : [ "cterms_data.csv","cleavages_data.csv","nterms_data.csv" ]
},
"preprocess": {
  "data": {
    "$[?(@.meropscode)].uniprot" : {
      "uniprot" : "@.substrate",
      "protease" : "@.protease",
      "position" : "@.position",
      "merops" : "@.meropscode",
      "meropsdb" : "true"
    },
    "$[?(! @.meropscode && @.methodology != 'unknown')].uniprot" : {
      "uniprot" : "@.substrate",
      "position" : "@.position",
      "methodology" : "@.methodology",
      "highthroughput" : "true"
    },
    "$[?(! @.meropscode && @.methodology == 'unknown')].uniprot" : {
      "uniprot" : "@.substrate",
      "position" : "@.position",
      "highthroughput" : "unknown"
    }
  }
},
"template" : {
  "data" : {
    "$.data.*[?(@.meropsdb)].uniprot" : {
      "uniprot" : "@.uniprot",
      "site" : "@.position",
      "protease" : "@.protease",
      "merops" : "@.merops"
    },
    "$.data.*[?(@.highthroughput == 'true')].uniprot" : {
      "uniprot" : "@.uniprot",
      "site" : "@.position",
      "method" : "@.methodology"
    },
    "$.data.*[?(@.highthroughput == 'unknown')].uniprot" : {
      "uniprot" : "@.uniprot",
      "site" : "@.position"
    }
  },
  "metadata": {
    "msdata-version" : "1.2",
    "title" : "TopFind database",
    "mimetype" : "application/json+cleavage"
  }
},
"environment" : {
}
}